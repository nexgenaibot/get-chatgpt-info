{
    "manifest_version": 3,
    "name": "ChatGPT 信息 - 测试版",
    "version": "1.7",
    "permissions": [
        "activeTab"
    ],
    "host_permissions": [
        "https://chatgpt.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}